<div>
     <table  class="board">
         <tr class="board">
             <th class="board">N°</th>
             <th class="board">Domicile</th>
             <th class="board">Extérieur</th>
             <th colspan=2 class="board">{{ app.request.get(app.request.get('_acc_1')) }}</th>
             <th colspan=2 class="board">{{ app.request.get(app.request.get('_acc_2')) }}</th>
             <th class="board">Score</th>
         </tr>
         {% for match in matchesA %}
         <tr class="vote-board">
             <td class="board">{{match.number}}</td>
             <td class="board">{{match.teamHome}}</td>
             <td class="board">{{match.teamVisitor}}</td>
             <td class="board">{{ app.request.get(match.id~'_result_1') }}</td>
             <td class="board">{{ app.request.get(match.id~'_score_1') }}</td>
             <td class="board">{{ app.request.get(match.id~'_result_2') }}</td>
             <td class="board">{{ app.request.get(match.id~'_score_2') }}</td>
             <td class="board">{{match.score}}</td>
          </tr>            
         {% endfor %}
     </table>
     <br/>
     
     {% if matchesR is defined and matchesR is not null %}
     
     <table  class="board">
         <tr>
             <th class="board">N°</th>
             <th class="board">Domicile</th>
             <th class="board">Extérieur</th>
             <th colspan=2 class="board">{{ app.request.get(app.request.get('_acc_1')) }}</th>
             <th colspan=2 class="board">{{ app.request.get(app.request.get('_acc_2')) }}</th>
             <th class="board">Score</th>
         </tr>
        {% for match in matchesR %}
         <tr class="vote-board">
             <td class="board">{{match.number}}</td>
             <td class="board">{{match.teamHome}}</td>
             <td class="board">{{match.teamVisitor}}</td>
             <td class="board">{{ app.request.get(match.id~'_result_1') }}</td>
             <td class="board">{{ app.request.get(match.id~'_score_1') }}</td>
             <td class="board">{{ app.request.get(match.id~'_result_2') }}</td>
             <td class="board">{{ app.request.get(match.id~'_score_2') }}</td>
             <td class="board">{{match.score}}</td>
          </tr>            
         {% endfor %}
     </table><br/>

     {% for match in matchesR %}
     {% if match.isFirstGoal %}
     <table  class="board">
         <tr>
             <th class="board">Golden Match</th>                               
             <th class="board">{{ app.request.get(app.request.get('_acc_1')) }}</th>
             <th class="board">{{ app.request.get(app.request.get('_acc_2')) }}</th>
             <th class="board">Minute du 1er But</th>
         </tr>
         <tr class="vote-board">
             <td class="board">{{match.teamHome}} - {{match.teamVisitor}}</td>
             <td class="board">{{ app.request.get(match.id~'_fgmin_1') }}</td>
             <td class="board">{{ app.request.get(match.id~'_fgmin_2') }}</td>
             <td class="board">{{match.firstGoalMin}}</td>
         </tr>
     </table>
     {% endif %}
     {% endfor %}
     
     {% endif %}
     
     {% for match in matchesA %}
     {% if match.isFirstGoal %}
     <table  class="board">
         <tr>
             <th class="board">Golden Match</th>                               
             <th class="board">{{ app.request.get(app.request.get('_acc_1')) }}</th>
             <th class="board">{{ app.request.get(app.request.get('_acc_2')) }}</th>
             <th class="board">Minute du 1er But</th>
         </tr>
         <tr class="vote-board">
             <td class="board">{{match.teamHome}} - {{match.teamVisitor}}</td>
             <td class="board">{{ app.request.get(match.id~'_fgmin_1') }}</td>
             <td class="board">{{ app.request.get(match.id~'_fgmin_2') }}</td>
             <td class="board">{{match.firstGoalMin}}</td>
         </tr>
     </table>
     {% endif %}
     {% endfor %}
 </div> 